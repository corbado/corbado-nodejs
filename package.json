{
  "name": "@corbado/node-sdk",
  "version": "2.0.0",
  "description": "This Node.js SDK eases the integration of Corbado's passkey-first authentication solution.",
  "module": "src/sdk.ts",
  "type": "module",
  "scripts": {
    "generate-client": "openapi-generator-cli generate -g typescript-axios -i ../corbado/backend/openapi/backend_api_public.yml -o src/sdk/entity/api",
    "test": "mocha --reporter spec",
    "build": "tsc"
  },
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corbado/corbado-nodejs.git"
  },
  "keywords": [
    "passkeys",
    "authentication",
    "webhooks",
    "fido2"
  ],
  "author": "Corbado",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/corbado/corbado-nodejs/issues"
  },
  "homepage": "https://github.com/corbado/corbado-nodejs#readme",
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "assert": "^2.1.0",
    "axios": "^1.6.2",
    "jose": "^5.1.3",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.4"
  }
}
