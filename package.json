{
  "name": "@corbado/node-sdk",
  "version": "1.1.16",
  "description": "This Node.js SDK eases the integration of Corbado's passkey-first authentication solution.",
  "keywords": [
    "passkeys",
    "authentication",
    "webhooks",
    "fido2"
  ],
  "homepage": "https://github.com/corbado/corbado-nodejs#readme",
  "bugs": {
    "url": "https://github.com/corbado/corbado-nodejs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corbado/corbado-nodejs.git"
  },
  "license": "MIT",
  "author": "Corbado",
  "type": "module",
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.js"
  },
  "main": "cjs/index.js",
  "module": "src/index.js",
  "scripts": {
    "prepublishOnly": "make build",
    "test": "make test",
    "lint": "make lint",
    "fix": "make fix",
    "tsc": "tsc",
    "start": "make start"
  },
  "dependencies": {
    "assert": "^2.0.0",
    "axios": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "jose": "^4.14.4",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.6",
    "@types/node-fetch": "^2.6.10",
    "@types/sinon": "^17.0.2",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "mocha": "^10.2.0",
    "sinon": "^15.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.1"
  },
  "files": [
    "package.json",
    "README.md",
    "esm",
    "cjs",
    "LICENSE",
    "src"
  ],
  "mocha": {
    "require": [
      "./test/polyfills.mjs"
    ],
    "loader": [
      "ts-node/esm"
    ],
    "recursive": true,
    "extension": [
      "ts",
      "js",
      "tsx"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ]
  }
}