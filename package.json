{
  "name": "@corbado/node-sdk",
  "version": "1.1.8",
  "description": "This Node.js SDK eases the integration of Corbado's passkey-first authentication solution.",
  "main": "dist/cjs/index.js",
  "module": "src/index.js",
  "type": "module",
  "scripts": {
    "build-es5": "babel src -d dist/cjs --presets=@babel/preset-env --plugins=@babel/plugin-transform-modules-commonjs",
    "build": "rm -fr dist/* && npm run build-es5 && ./fixup",
    "test": "mocha --reporter spec"
  },
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corbado/corbado-nodejs.git"
  },
  "keywords": [
    "passkeys",
    "authentication",
    "webhooks",
    "fido2"
  ],
  "author": "Corbado",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/corbado/corbado-nodejs/issues"
  },
  "homepage": "https://github.com/corbado/corbado-nodejs#readme",
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.22.5",
    "assert": "^2.0.0",
    "axios": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "jose": "^4.14.4",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "mocha": "^10.2.0",
    "sinon": "^15.0.4"
  }
}
